:80 {
    route {
        header {
            Access-Control-Allow-Origin *
            X-Frame-Options "Deny"
            Content-Security-Policy "script-src 'self' https://challenges.cloudflare.com;frame-src 'self' https://challenges.cloudflare.com;frame-ancestors 'none';"
        }
    
        root * /srv/
        encode gzip
        file_server
    }
}
